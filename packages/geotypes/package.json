{
  "name": "@jsse/geotypes",
  "version": "0.0.2",
  "description": "geo types",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./pacakge.json": "./package.json"
  },
  "scripts": {
    "build-fast": "tsup src/geojson.ts --format cjs,esm",
    "build": "pnpm run build:indexes && pnpm run build:tsup",
    "build:indexes": "tsx ./build.mts",
    "build:tsup": "tsup --format cjs,esm --dts",
    "test": "vitest run",
    "dev": "vitest watch --coverage",
    "lint": "eslint src/** --ext .ts",
    "fmt": "prettier --write .",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/geojson": "^7946.0.11",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitest/coverage-v8": "^0.34.6",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "prettier": "3.0.3",
    "tsup": "7.2.0",
    "tsx": "^3.13.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6",
    "zx": "^7.2.3"
  }
}
