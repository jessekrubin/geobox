{
  "name": "@jsse/geojson",
  "version": "0.0.1",
  "description": "geojson",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/geojson.cjs",
  "module": "./dist/geojson.js",
  "exports": {
    ".": {
      "require": "./dist/geojson.js",
      "import": "./dist/geojson.mjs",
      "types": "./dist/geojson.d.ts"
    },
    "./pacakge.json": "./package.json"
  },
  "types": "./dist/geojson.d.ts",
  "scripts": {
    "build-fast": "tsup src/geojson.ts --format cjs,esm",
    "build": "tsup src/geojson.ts --format cjs,esm --dts",
    "test": "vitest run",
    "dev": "vitest watch --coverage",
    "lint": "eslint src/** --ext .ts",
    "fmt": "prettier --write .",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/geojson": "^7946.0.10",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/coverage-v8": "^0.32.2",
    "eslint": "^8.43.0",
    "prettier": "2.8.8",
    "tsup": "6.7.0",
    "tsx": "^3.12.7",
    "typescript": "5.1.3",
    "vitest": "0.32.2"
  }
}
